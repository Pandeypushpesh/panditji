{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/styles.css",
      "dest": "/api/index.js"
    },
    {
      "src": "/script.js",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(css|js|json|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|webp))",
      "dest": "/api/index.js"
    },
    {
      "src": "/lang/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}

